#include <stdint.h>

#ifndef MAIN_H_
#define MAIN_H_

typedef struct
{
	uint32_t GPIO_AEN:1;
	uint32_t GPIO_BEN:1;
	uint32_t GPIO_CEN:1;
	uint32_t GPIO_DEN:1;
	uint32_t GPIO_EEN:1;
	uint32_t GPIO_FEN:1;
	uint32_t GPIO_GEN:1;
	uint32_t GPIO_HEN:1;
	uint32_t GPIO_IEN:1;
	uint32_t GPIO_JEN:1;
	uint32_t GPIO_KEN:1;
	uint32_t RESERVED_0:1;
	uint32_t CRCEN:1;
	uint32_t RESERVED_1:5;
	uint32_t BKPSR_AMEN:1;
	uint32_t RESERVED_2:1;
	uint32_t CCMDAT_ARAMEN:1;
	uint32_t DMA1_EN:1;
	uint32_t DMA2_EN:1;
	uint32_t DMA2_DEN:1;
	uint32_t RESERVED_3:1;
	uint32_t ETHMAC_EN:1;
	uint32_t ETHMAC_TXEN:1;
	uint32_t ETHMAC_RXE_N:1;
	uint32_t ETHMAC_PTP_EN:1;
	uint32_t OTGHSEN:1;
	uint32_t OTGHSULPIEN:1;
	uint32_t RESERVED_4:1;

}RCC_AHB1ENR_t;

typedef struct
{
	uint32_t ODR0:1;
	uint32_t ODR1:1;
	uint32_t ODR2:1;
	uint32_t ODR3:1;
	uint32_t ODR4:1;
	uint32_t ODR5:1;
	uint32_t ODR6:1;
	uint32_t ODR7:1;
	uint32_t ODR8:1;
	uint32_t ODR9:1;
	uint32_t ODR10:1;
	uint32_t ODR11:1;
	uint32_t ODR12:1;
	uint32_t ODR13:1;
	uint32_t ODR14:1;
	uint32_t ODR15:1;
	uint32_t RESERVED:16;
}GPIOx_ODR;

typedef struct
{
	uint32_t MODE0:2;
	uint32_t MODE1:2;
	uint32_t MODE2:2;
	uint32_t MODE3:2;
	uint32_t MODE4:2;
	uint32_t MODE5:2;
	uint32_t MODE6:2;
	uint32_t MODE7:2;
	uint32_t MODE8:2;
	uint32_t MODE9:2;
	uint32_t MODE10:2;
	uint32_t MODE11:2;
	uint32_t MODE12:2;
	uint32_t MODE13:2;
	uint32_t MODE14:2;
	uint32_t MODE15:2;

}GPIOx_MODE_t;

#endif /* MAIN_H_ */
